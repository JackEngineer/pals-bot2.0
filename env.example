# Telegram Bot 配置
TELEGRAM_BOT_TOKEN=YOUR_TELEGRAM_BOT_TOKEN

# 应用配置
NEXT_PUBLIC_APP_URL=https://your-app-domain.vercel.app

# Recommended for most uses
DATABASE_URL=postgres://username:password@your-host-pooler.region.aws.neon.tech/dbname?sslmode=require

# For uses requiring a connection without pgbouncer
DATABASE_URL_UNPOOLED=postgresql://username:password@your-host.region.aws.neon.tech/dbname?sslmode=require

# Parameters for constructing your own connection string
PGHOST=your-host-pooler.region.aws.neon.tech
PGHOST_UNPOOLED=your-host.region.aws.neon.tech
PGUSER=your_username
PGDATABASE=your_database
PGPASSWORD=your_password

# Parameters for Vercel Postgres Templates
POSTGRES_URL=postgres://username:password@your-host-pooler.region.aws.neon.tech/dbname?sslmode=require
POSTGRES_URL_NON_POOLING=postgres://username:password@your-host.region.aws.neon.tech/dbname?sslmode=require
POSTGRES_USER=your_username
POSTGRES_HOST=your-host-pooler.region.aws.neon.tech
POSTGRES_PASSWORD=your_password
POSTGRES_DATABASE=your_database
POSTGRES_URL_NO_SSL=postgres://username:password@your-host-pooler.region.aws.neon.tech/dbname
POSTGRES_PRISMA_URL=postgres://username:password@your-host-pooler.region.aws.neon.tech/dbname?connect_timeout=15&sslmode=require

# Neon Auth environment variables for Next.js
NEXT_PUBLIC_STACK_PROJECT_ID=your-stack-project-id
NEXT_PUBLIC_STACK_PUBLISHABLE_CLIENT_KEY=your-stack-publishable-client-key
STACK_SECRET_SERVER_KEY=your-stack-secret-server-key